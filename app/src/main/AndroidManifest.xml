<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeanwest.reader">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Handheld">

        <activity android:name=".transference.TransferScanningLogin"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".transference.TransferScanningActivity"
            android:parentActivityName=".transference.TransferScanningLogin"/>

        <activity android:name=".transference.TransferScanningResultActivity"
            android:parentActivityName=".transference.TransferScanningActivity"/>

        <activity android:name=".transference.TransferenceActivity"
            android:parentActivityName=".transference.TransferenceActivityLogIn"/>

        <activity android:name=".transference.TransferenceActivityLogIn"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".UserLoginActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".finding.FindingProductSubActivity"
            android:parentActivityName=".finding.FindingProductActivity" />
        <activity
            android:name=".finding.FindingProductActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".warehouseScanning.WarehouseScanningSubResultActivity"
            android:parentActivityName=".warehouseScanning.WarehouseScanningResultActivity" />
        <activity
            android:name=".SettingActivity"
            android:parentActivityName=".AboutUsActivity" />
        <activity
            android:name=".warehouseScanning.WarehouseScanningResultActivity"
            android:parentActivityName=".warehouseScanning.WarehouseScanningActivity" />
        <activity
            android:name=".warehouseScanning.WarehouseScanningUserLogin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginToSettingActivity"
            android:parentActivityName=".AboutUsActivity" />
        <activity
            android:name=".AboutUsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".warehouseScanning.WareHouseScanningFindingProduct"
            android:parentActivityName=".warehouseScanning.WarehouseScanningSubResultActivity" />
        <activity
            android:name=".warehouseScanning.WarehouseScanningActivity"
            android:parentActivityName=".warehouseScanning.WarehouseScanningUserLogin" />
        <activity
            android:name=".add.AddProductActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>