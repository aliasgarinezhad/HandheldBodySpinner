<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jeanwest.reader">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Handheld">
        <activity android:name=".UserLoginActivity"></activity>
        <activity
            android:name=".FindingProductSubActivity"
            android:parentActivityName=".FindingProductActivity" />
        <activity
            android:name=".FindingProductActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WarehouseScanningSubResultActivity"
            android:parentActivityName=".WarehouseScanningResultActivity" />
        <activity
            android:name=".SettingActivity"
            android:parentActivityName=".AboutUsActivity" />
        <activity
            android:name=".WarehouseScanningResultActivity"
            android:parentActivityName=".WarehouseScanningActivity" />
        <activity
            android:name=".WarehouseScanningUserLogin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LoginToSettingActivity"
            android:parentActivityName=".AboutUsActivity" />
        <activity
            android:name=".AboutUsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WareHouseScanningFindingProduct"
            android:parentActivityName=".WarehouseScanningSubResultActivity" />
        <activity
            android:name=".WarehouseScanningActivity"
            android:parentActivityName=".WarehouseScanningUserLogin" />
        <activity
            android:name=".AddProductActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>